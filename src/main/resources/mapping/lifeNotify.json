            },
            "status": {
                "type": "integer"
            },
            "articleId": {
                "type": "integer"
            },
            "createUserId": {
                "type": "integer"
            },
            "createUserTruename": {
                "type": "text"
            },
            "createTime": {
                "format": "yyyy-MM-dd HH:mm:ss",
                "type": "date"
            },
            "publishStatus": {
                "type": "integer"
            }
        }
    }
}